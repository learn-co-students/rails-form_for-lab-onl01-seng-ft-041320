<h3>Edit Class Form</h3>

<%= form_for @school_class do |f| %>
  <%= f.label 'Title' %><br>
  <%= f.text_field :title %><br>

  <%= f.label 'Room Number' %><br>
  <%= f.number_field :room_number %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.submit %>
<% end %>
