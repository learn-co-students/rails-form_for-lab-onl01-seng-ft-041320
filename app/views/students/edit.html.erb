<h3>Edit Student Form</h3>

<%= form_for @student do  |f| %>
  <%= f.label 'First Name' %><br>
  <%= f.text_field :first_name %><br>

  <%= f.label 'Last Name' %><br>
  <%= f.text_field :last_name %><br>

  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= f.submit %>
<% end %>
